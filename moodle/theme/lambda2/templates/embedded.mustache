{{{ output.standard_top_of_body_html }}}
<div id="page" {{#hasfakeblocks}}class="has-fake-blocks"{{/hasfakeblocks}}>
    {{#hasfakeblocks}}
        <section class="embedded-blocks" aria-label="{{#str}}blocks{{/str}}">
            {{{ fakeblocks }}}
